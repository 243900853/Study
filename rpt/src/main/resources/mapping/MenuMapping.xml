<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaobi.mapper.MenuMapper">

    <resultMap id="BaseResultMap" type="com.xiaobi.bean.Menu">
        <result column="menuId" jdbcType="INTEGER" property="menu_id" />
        <result column="name" jdbcType="VARCHAR" property="name" />
        <result column="url" jdbcType="VARCHAR" property="url" />
    </resultMap>

    <select id="queryMenuByLevel" resultType="com.xiaobi.bean.Menu">
        select * from menu where level = #{level}
    </select>

</mapper>